import java.util.*;
public class nopdbc 
{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int noq=N/5;
        System.out.println("No of questions done by coders="+noq);
        System.out.println("No of questions unsolved by coders="+(N%5));
    }
}
